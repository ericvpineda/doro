{
    "name": "Doro - Pomodoro with Spotify Player",
    "version": "1.0.0",
    "description": "Pomodoro with spotify integration extension.",
    "manifest_version": 3,
    "author": "Eric Pineda",
    "action": {
        "default_title": "Doro",
        "default_popup": "index.html"
    }, 
    "permissions": ["storage", "alarms"],
    "background": {
        "service_worker": "popup.js"
    }
}